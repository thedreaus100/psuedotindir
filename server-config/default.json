{
    "redis": {
        "host": "localhost",
        "port": "6379",
        "options": {}
    },
    "couchdb": {
        "host": "localhost",
        "port": "5984",
        "database": "psuedotindir"
    },
    "elastic": {
        "host":"localhost",
        "port":9200,
        "default_index":"psuedotindir"
